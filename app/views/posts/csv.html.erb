<div class="container ">
  <div class="row justify-content-center">
    <div class="col-5">
      <h1>CSV Upload</h1>
      <%= form_with do |form|%>
        <%= form.label :csv , "CSV File :"%>
        <%= form.file_field :csv %><br/><br/>

        <%= form.submit "Upload"%>
      <% end %>
    </div>
  </div>
</div>